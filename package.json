{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "loafy",
  "version": "0.0.0",
  "description": "Build your full-stack project easier and faster",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "type": "module",
  "keywords": [
    "cli",
    "template",
    "nextjs",
    "drizzle",
    "supabase",
    "neon",
    "shadcn",
    "better-auth"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hexaaagon/loafy.git"
  },
  "bugs": {
    "url": "https://github.com/hexaaagon/loafy/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.14.0"
}
